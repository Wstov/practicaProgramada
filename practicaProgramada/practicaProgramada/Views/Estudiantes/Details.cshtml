@model PracticaProgramada.BLL.Dtos.EstudianteDto

<h2>Detalle del Estudiante</h2>

<table class="table">
    <tr><th>ID:</th><td>@Model.Id</td></tr>
    <tr><th>Nombre:</th><td>@Model.Nombre</td></tr>
    <tr><th>Apellido:</th><td>@Model.Apellido</td></tr>
    <tr><th>Carné:</th><td>@Model.Carne</td></tr>
    <tr><th>Email:</th><td>@Model.Email</td></tr>
    <tr><th>Teléfono:</th><td>@Model.Telefono</td></tr>
</table>

<a href="/Estudiantes" class="btn btn-secondary">Volver</a>
